{
    "name": "neilime/zf2-ckeditor-bundle",
    "type": "library",
    "description": "Zend Framework 2 module for easy integration of the CKEditor WYSIWYG",
    "keywords": ["CKEditor","bundle"],
    "homepage": "",
    "license": "MIT",
    "minimum-stability" : "stable",
    "authors": [
        {
            "name": "Neilime",
            "homepage": "http://neilime.github.com/zf2-ckeditor-bundle",
            "role": "Developer"
        }
    ],
    "repositories":[
        {
        "type": "git",
        "url": "https://github.com/ckeditor/ckeditor-releases"
        }
    ],
    "suggest": {
        "neilime/zf2-assets-bundle": "Bundling & caching CKEditor assets"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
        "phpunit/phpunit": "3.7.*",
        "ckeditor/ckeditor": "dev-full/4.3.x"
    },
    "autoload": {
        "psr-0": {
            "CKEditorBundle": "src"
        },
        "classmap" : ["Module.php"]
    }
}
